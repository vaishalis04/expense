    margin-left: 45vh;
   
}

@media (max-width: 600px) {
    .nav {
        justify-content: end;
        font-size: 3vmax;
    }
    .nav > span {
        display: initial;
    }
    .container1 > .nav > li {
        display: none;
    }

    .container1 > .content {
        height: 70vmax;
    }
    .cards{
        display: block;
      
    }
    .card{
        width: 50vh;
        margin-bottom: 5vh;
    }
    .see{
        margin-top: 120vh;
        text-align: center;
    }
    .p3-cards{
        display: block;
    }
    .p3-contents {
        
        display: block;
    }
    .p3-card{
        width: 50vh;
        margin-bottom: 5vh;
    }
    .content1{
        width: 50vh;
        margin-bottom: 5vh;
    }
    .content2{
        width: 60vh;
    }
    .card1,.card2, .card3{
        margin-left: 50px;
    }
    .p3-card1,  .p3-card2{
        margin-left: 50px;

    }
    .post{
        margin-top: 90vh;
        text-align: center;
    }
    .photo{
        padding-top: 50%;
    }
    .footer > ul{
 
        margin-left: -19vh;
        font-size: 65%;
    }

}

</style>
